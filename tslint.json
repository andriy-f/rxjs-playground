{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "indent": [true, "spaces", 2],
      "quotemark": [true, "single", "avoid-escape", "avoid-template"],
      "semicolon": [true, "never"],
      "linebreak-style": [true, "LF"],
      "no-console": true
    },
    "rulesDirectory": []
}
